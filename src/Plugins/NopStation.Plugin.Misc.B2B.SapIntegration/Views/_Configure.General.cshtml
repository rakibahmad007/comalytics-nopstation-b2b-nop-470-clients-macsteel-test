@model ConfigurationModel

<div class="card-body">
    <div class="card card-default">
        <div class="card-body">
            <div class="form-group row">
                <div class="col-md-3">
                    <nop-label asp-for="HttpCallMaxRetries" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="HttpCallMaxRetries" />
                    <span asp-validation-for="HttpCallMaxRetries"></span>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-3">
                    <nop-label asp-for="HttpCallRestTimeInSeconds" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="HttpCallRestTimeInSeconds" />
                    <span asp-validation-for="HttpCallRestTimeInSeconds"></span>
                </div>
            </div>

			<div class="form-group row">
				<div class="col-md-3">
					<nop-label asp-for="IntegrationSecretKey" />
				</div>
				<div class="col-md-9">
					<nop-editor asp-for="IntegrationSecretKey" />
					<span asp-validation-for="IntegrationSecretKey"></span>
				</div>
			</div>
        </div>
    </div>
</div>