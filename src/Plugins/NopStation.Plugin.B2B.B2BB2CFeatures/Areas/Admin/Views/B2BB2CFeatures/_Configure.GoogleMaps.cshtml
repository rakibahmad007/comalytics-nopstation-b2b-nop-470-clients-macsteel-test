@model ConfigurationModel

<div class="card-body">
    <div class="form-group row">
        <div class="col-md-3">
            <nop-label asp-for="GoogleMapsApiKey" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="GoogleMapsApiKey" />
            <span asp-validation-for="GoogleMapsApiKey"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <nop-label asp-for="Latitude" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="Latitude" />
            <span asp-validation-for="Latitude"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <nop-label asp-for="Longitude" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="Longitude" />
            <span asp-validation-for="Longitude"></span>
        </div>
    </div>
</div>